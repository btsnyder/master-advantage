@page "/character"


<PageTitle>Character</PageTitle>

<MudText Typo="Typo.h3" GutterBottom="true">Characters</MudText>

<MudButton Color="Color.Primary" Variant="Variant.Filled" @onclick="IncrementCount" Href="character/create">Create New</MudButton>

@code {
    private int currentCount = 0;

    private void IncrementCount()
    {
        currentCount++;
    }
}
